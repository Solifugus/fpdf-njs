{
  "name": "fpdf-njs",
  "version": "2.0.0",
  "description": "Lightweight PDF generation for Node.js. Port of FPDF with zero eval(), built-in compression, JPEG support, and comprehensive testing. Minimal dependencies.",
  "main": "fpdf.js",
  "scripts": {
    "test": "node test/run-all.js",
    "test:lib": "node test/lib.test.js",
    "test:core": "node test/core.test.js",
    "example": "node example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solifugus/fpdf-njs.git"
  },
  "keywords": [
    "pdf",
    "pdf-generation",
    "fpdf",
    "document",
    "compression",
    "jpeg",
    "image",
    "nodejs",
    "lightweight"
  ],
  "author": "Matthew C. Tedder <matthewct@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Solifugus/fpdf-njs/issues"
  },
  "homepage": "https://github.com/Solifugus/fpdf-njs#readme",
  "dependencies": {
    "sprintf-js": "^1.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
